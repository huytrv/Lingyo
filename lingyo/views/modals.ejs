<!-- post-content -->
<div class='post'><div class='post-util'><button class='avt-but header-but dropdown-but util-dropdown-but' onclick='dropdownToggle(this)'><span class='iconify dropdown-icon bg-white' data-icon='vaadin:ellipsis-dots-h' data-inline='false'></span></button><div class='util-dropdown-content dropdown-content'><a class='nav-item'><span class='iconify' data-icon='bi:bookmark-plus' data-inline='false'></span>Lưu bài viết</a><a class='nav-item'><span class='iconify' data-icon='clarity:bell-outline-badged' data-inline='false'></span>Bật thông báo</a><a class='nav-item'><span class='iconify' data-icon='octicon:issue-opened' data-inline='false'></span>Báo cáo bài viết</a><a class='nav-item'><span class='iconify' data-icon='bx:bx-hide' data-inline='false'></span>Ẩn bài viết</a></div></div><div class='post-user'><div class='post-info'><a class='avt' href='/personal'><img src='/public/images/avt.jpg'></a><div class='avt-info'><a class='avt' href='/personal'><span class='avt-username'>${res.data.username}</span></a><a class='avt' href='/personal'><span class='post-time'>${res.data.nickname}</span></a></div><div class='mark-icon'><span class='iconify' data-icon='mdi:checkbox-marked-circle' data-inline='false'></span></div><div class='mg-l'><span class='contact-item'></span><a class='avt' href='/personal'><span class='post-time theme-color'>Vừa xong</span></a></div> </div></div><div class='post-content'><p class='post-description'>${description}</p>${(() => {if(files) {if(vdOrImg = 'video'){return '<div class='post-file'><video class='player'><source src='<%- post.file.path -%>'></video></div>'}else{if (files.path.length == 1) {return '<div class='post-file'><img src='<%- post.file.path[0] -%>'></div>'}if (files.path.length == 2) {return '<div class='post-file thumb-2-files'><div class='thumb-file'><img src='<%- post.file.path[0] -%>'></div><div class='thumb-file'><img src='<%- post.file.path[1] -%>'></div></div>'}if (files.path.length == 3) {return '<div class='post-file thumb-3-files'><div class='thumb-file'><img src='<%- post.file.path[0] -%>'></div><div class='thumb-file'><img src='<%- post.file.path[1] -%>'></div><div class='thumb-file'><img src='<%- post.file.path[2] -%>'></div></div>'}if (post.file.path.length == 4) {return '<div class='post-file thumb-4-files'><div class='thumb-file'><img src='<%- post.file.path[0] -%>'></div><div class='thumb-file'><img src='<%- post.file.path[1] -%>'></div><div class='thumb-file'><img src='<%- post.file.path[2] -%>'></div><div class='thumb-file'><img src='<%- post.file.path[3] -%>'></div></div>'}}} else {return nope}})()}</div><div class='post-interactive'><button class='interactive-but'><span class='iconify font-size-lg-1' data-icon='simple-line-icons:like' data-inline='false'></span><span></span></button><button class='interactive-but' onclick='postDetail()'><span class='iconify font-size-lg-1' data-icon='bi:chat-square' data-inline='false'></span><span></span></button><button class='interactive-but'><span class='iconify font-size-lg-1' data-icon='simple-line-icons:share-alt' data-inline='false'></span><span></span></button></div></div>

