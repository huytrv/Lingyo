<!DOCTYPE html>
<html>
<head>
    <% if (cateName == '') { %>
        <title>Fodance</title>
    <% } %>
    <% if (cateName != '') { %>
        <title>Fodance | <%- cateName.charAt(0).toUpperCase() + cateName.slice(1) %></title>
    <% } %>
    <%- include('header') -%>
</head>
<body>
    <div class="global">
        <div class="header">
            <div class="header-inner-mobile">
                <div class="d-flex-start">
                    <div class="sidenav-frame">
                        <div class="sidenav">
                            <a class="closebtn close-nav">&times;</a>
                            <a class="nav-item nav-red avt header-nickname noselect mg-r" nav-data='personal' data-user-df="<%- profile.nickname -%>">
                                <% if (profile.avatar.includes("http")) { %>
                                    <img src="<%- profile.avatar -%>" class="user-avt header-avatar" username="<%- username -%>">
                                    <% } %>
                                <% if (!profile.avatar.includes("http")) { %>
                                    <img src="https://cdn.fodance.com/<%- profile.avatar -%>" class="user-avt header-avatar" username="<%- username -%>">
                                    <% } %>
                                <div class="d-flex-col-start">
                                    <span class="avt-username header-username nav-item-name-red none-deco user-username"><% if (username.length > 15) { %><%- username.substring(0, 12) + '...' + username.substring(username.length) -%><% } %><% if (username.length <= 15) { %><%- username -%><% } %></span>
                                    <span class="avt-nickname avatar-nickname">@<%- profile.nickname -%></span>
                                </div>
                            </a>
                            <div class="d-flex-start pd">
                                <div class="header-dp d-flex user-stars noselect">
                                    <span class="iconify font-icon mg-l-sm theme-color" data-icon="emojione-v1:money-bag" data-inline="false"></span>
                                    <span class="star-total"><%- profile.stars -%></span>
                                </div>
                                <div class="header-dp d-flex user-tickets noselect mg-r-lg">
                                    <span class="iconify font-icon mg-r-sm red-color" data-icon="foundation:ticket" data-inline="false"></span>
                                    <span class="ticket-total"><%- profile.tickets -%></span>
                                </div>
                            </div>
                            <div class="search-but pd">
                                <span class="iconify icon" data-icon="heroicons-outline:search" data-inline="false"></span>
                                <form autocomplete="off" class="search-form">
                                    <input type="text" name="search" class="home-input search-input" placeholder="Tìm kiếm trên Fodance" maxlength="30">   
                                </form>
                            </div>
                            <div class="border-b mg-l mg-r"></div>
                            <div class="d-flex">
                                <a class="sort-content-but d-flex-col noselect sort-cate <% if (cateActive != '') { %>sort-content-but-active <% } %>" sort-data="rank-sort-content"><span class="iconify" data-icon="bi:bar-chart-fill" data-inline="false"></span><div class="d-flex-col"><span>Thứ</span><span>hạng</span></div></a>
                                <a class="sort-content-but d-flex-col noselect" sort-data='random-sort-content'><span class="iconify" data-icon="fe:random" data-inline="false"></span><div class="d-flex-col"><span>Ngẫu</span><span>nhiên</span></div></a>
                                <a class="sort-content-but d-flex-col noselect" sort-data='latest-sort-content'><span class="iconify" data-icon="ic:twotone-autorenew" data-inline="false"></span><div class="d-flex-col"><span>Mới</span><span>nhất</span></div></a>
                                <a class="sort-content-but d-flex-col noselect" sort-data='follow-sort-content'><span class="iconify" data-icon="ri:user-follow-line" data-inline="false"></span><div class="d-flex-col"><span>Theo</span><span>dõi</span></div></a>
                            </div>
                            <div class="border-b mg-l mg-r"></div>
                            <a class="nav-item d-flex-start noselect filter-but dropdown-but mg-t">
                                <span class="iconify dropdown-icon mg-r-sm" data-icon="whh:sortbysizedescending" data-inline="false"></span><div class="dropdown-text"><span>Lọc</span></div>
                            </a>
                            <a class="nav-item nav-red honors-but <% if (active == 'honors') { %> active <% } %>" nav-data='honors'><span class="iconify" data-icon="ph:medal-light" data-inline="false"></span><span class="nav-item-name-red">Vinh danh</span></a>
                            <a class="nav-item nav-red saved-but <% if (active == 'saved') { %> active <% } %>" nav-data='saved'><span class="iconify" data-icon="bi:bookmark-check" data-inline="false"></span><span class="nav-item-name-red">Đã lưu</span></a>
                            <a class="nav-item nav-red add-topic-but <% if (active == 'add-topic') { %> active <% } %>" nav-data='add-topic'><span class="iconify" data-icon="heroicons-outline:view-grid-add" data-inline="false"></span><span class="nav-item-name-red">Thêm thể loại</span></a>
                            <div class="border-b mg-l mg-r"></div>
                            <a class="nav-item d-flex-start nav-red" nav-data="setting" setting-data="feedback"><span class="iconify" data-icon="bx:bx-layer-plus" data-inline="false"></span><span class="nav-item-name-red">Ý kiến và phản hồi</span></a>
                            <a class="nav-item d-flex-start nav-red" nav-data="setting" setting-data="normal"><span class="iconify" data-icon="cil:settings" data-inline="false"></span><span class="nav-item-name-red">Cài đặt</span></a>
                            <div class="border-b mg-l mg-r"></div>
                            <a href="/login" class="nav-item mg-b d-flex-start"><span class="iconify" data-icon="ant-design:logout-outlined" data-inline="false"></span><span class="nav-item-name-red">Đăng suất</span></a>
                        </div>
                    </div> 
                    <div class="avt open-nav">
                        <% if (profile.avatar.includes("http")) { %>
                            <img src="<%- profile.avatar -%>" class="user-avt mg-l-sm" username="<%- username -%>">
                            <% } %>
                        <% if (!profile.avatar.includes("http")) { %>
                            <img src="https://cdn.fodance.com/fd-media/<%- profile.avatar -%>" class="user-avt mg-l-sm" username="<%- username -%>">
                            <% } %>
                    </div>
                    <div class="title"><h2 class="title-content"></h2></div>
                </div>
                <div class="header-rank d-flex">
                    <button class="header-rank-but d-flex mg-r <% if (rankLink == 'primary') { %> header-rank-but-active <% } %>" rank-data="primary"><span class="iconify mg-r-sm" data-icon="uil:medal" data-inline="false"></span><span class="header-rank-but-name">Sơ cấp</span><span class="rank-post-count"></span></button>
                    <button class="header-rank-but mg-r d-flex <% if (rankLink == 'intermediate') { %> header-rank-but-active <% } %>" rank-data="intermediate"><span class="iconify mg-r-sm" data-icon="la:medal-solid" data-inline="false"></span><span class="header-rank-but-name">Trung cấp</span><span class="rank-post-count"></span></button>
                    <button class="header-rank-but d-flex <% if (rankLink == 'highgrade') { %> header-rank-but-active <% } %>" rank-data="highgrade"><span class="iconify mg-r-sm" data-icon="emojione-monotone:sports-medal" data-inline="false"></span><span class="header-rank-but-name">Cao cấp</span><span class="rank-post-count"></span></button>
                </div>
            </div>
            <div class="header-inner">
                <div class="logo">
                    <a class="logo-but nav-red" nav-data=''><img src="/public/images/logo/logo1.png" class="cf-img"><span class="nav-item-name-red"></span></a>   
                    <div class="search-but">
                        <span class="iconify icon" data-icon="heroicons-outline:search" data-inline="false"></span>
                        <form autocomplete="off" class="search-form">
                            <input type="text" name="search" class="home-input search-input" placeholder="Tìm kiếm trên Fodance" maxlength="30">   
                        </form>
                    </div> 
                </div>   
                <div class="header-rank">
                    <button class="header-rank-but d-flex mg-r <% if (rankLink == 'primary') { %> header-rank-but-active <% } %>" rank-data="primary"><span class="iconify mg-r-sm" data-icon="uil:medal" data-inline="false"></span><span class="header-rank-but-name">Sơ cấp</span><span class="rank-post-count"></span></button>
                    <button class="header-rank-but mg-r d-flex <% if (rankLink == 'intermediate') { %> header-rank-but-active <% } %>" rank-data="intermediate"><span class="iconify mg-r-sm" data-icon="la:medal-solid" data-inline="false"></span><span class="header-rank-but-name">Trung cấp</span><span class="rank-post-count"></span></button>
                    <button class="header-rank-but d-flex <% if (rankLink == 'highgrade') { %> header-rank-but-active <% } %>" rank-data="highgrade"><span class="iconify mg-r-sm" data-icon="emojione-monotone:sports-medal" data-inline="false"></span><span class="header-rank-but-name">Cao cấp</span><span class="rank-post-count"></span></button>
                </div>
                <div class="d-flex mg-r">
                    <div class="personal">
                        <a class="avt-header nav-red avt avt-but header-nickname d-flex noselect mg-r" nav-data='personal' data-user-df="<%- profile.nickname -%>">
                            <% if (profile.avatar.includes("http")) { %>
                                <img src="<%- profile.avatar -%>" class="user-avt header-avatar" username="<%- username -%>">
                                <% } %>
                            <% if (!profile.avatar.includes("http")) { %>
                                <img src="https://cdn.fodance.com/fd-media/<%- profile.avatar -%>" class="user-avt header-avatar" username="<%- username -%>">
                                <% } %>
                            <span class="avt-username header-username nav-item-name-red none-deco user-username"><% if (username.length > 15) { %><%- username.substring(0, 12) + '...' + username.substring(username.length) -%><% } %><% if (username.length <= 15) { %><%- username -%><% } %></span>
                        </a>
                    </div>
                    <div class="header-dp user-stars noselect mg-r">
                        <span class="star-total"><%- profile.stars -%></span>
                        <span class="iconify font-icon mg-l-sm theme-color" data-icon="emojione-v1:money-bag" data-inline="false"></span>
                    </div>
                    <div class="header-dp user-tickets noselect mg-r">
